            "terminal": true,
            "while": "posix-test"
        },
        {
            "case": "pthread",
            "command": "application.run:path=posix-test",
            "arguments": "'--api --pthread'",
            "terminal": true,
            "while": "posix-test"
        },
        {
            "case": "directory",
            "command": "application.run:path=posix-test",
