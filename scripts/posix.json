{
    "posix-test": [
        {
            "case": "configure",
            "command": "os.configure:time",
            "assert": false,
            "fatal": false
        },
        {
            "case": "build",
            "command": "app.build:path=posix-test,clean"
        },
        {
            "case": "build-slave",
            "command": "app.build:path=launchslave,clean"
        },
        {
            "case": "install",
            "command": "app.install:path=posix-test"
        },
        {
            "case": "install-slave",
            "command": "app.install:path=launchslave"
        },
        {
            "case": "stdio",
            "command": "application.run:path=posix-test",
            "arguments": "'--api --stdio'",
            "terminal": true,
            "while": "posix-test"
        },
        {
            "case": "aio",
            "command": "application.run:path=posix-test",
            "arguments": "'--api --aio'",
            "terminal": true,
            "while": "posix-test"
        },
        {
            "case": "sem",
